<h1>Sign Up</h1>
<%= form_with(model:@user,local:true)do |f|%>
  <%= f.label :namae %>
  <%= f.text_field :namae %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password%>
  <%= f.label :password_confirmation%>
  <%= f.password_field :password_confirmation%>
  <%= f.submit "Create my account" %>
<% end %>